    <!-- Images -->

    <Style x:Key="ToolBarCloseImage" TargetType="Rectangle">
        <Setter Property="Fill" Value="{DynamicResource WindowButtonForeBrush}"/>
        <Setter Property="OpacityMask">
            <Setter.Value>
                <VisualBrush
                    Stretch="Uniform"
                    Visual="{DynamicResource CloseIconVector}"
                    />
            </Setter.Value>
        </Setter>
    </Style>
    
    <Style x:Key="ToolBarMinimizeImage" TargetType="Rectangle">
        <Setter Property="Fill" Value="{DynamicResource WindowButtonForeBrush}"/>
        <Setter Property="OpacityMask">
            <Setter.Value>
                <VisualBrush
                    Stretch="Uniform"
                    Visual="{DynamicResource MinimizeIconVector}"
                    />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ToolBarOnTopImage" TargetType="Rectangle">
